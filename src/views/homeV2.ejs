				>
					<div class="col-lg-4 col-12 ms-lg-5 ms-0 py-2">
						<div class="fs-3 fw-bold"><span class="position-relative underline-overall-black"
							>SUBCHAPTERS</span></div>
					</div>
				</div>

				<div class="row text-center gx-3" id="xx">
					<div
						class="
							col-12 col-lg-9
							order-lg-first order-last
							my-2
							position-relative
						"
					>
						<div
							class="
								subImgv1
								position-absolute
								d-none d-lg-block
								py-3
							"
						></div>
						<div
							class="
								subImgv2
								position-absolute
								d-none d-lg-block
								py-3
							"
						></div>

						<img
							src="/images/subChp/sub-1.png"
							class="
								img-fluid
								sub-Img
								animate__animated animate__pulse
							"
							alt="subChp"
							id="home-subImg"
						/>
					</div>
					<div
						class="
							col-12 col-lg-3
							order-lg-last order-first
							position-relative
						"
					>
						<div class="container">
							<div class="row row-cols-4 mt-lg-5 text-white">
								<div
									class="
										col-lg-12 col-3
										p-2
										m-lg-3
										fs-6
										fw-bold
										subName1
									"
									id="wie"
								><div
								class="
									subv1
									position-absolute
									d-none d-lg-block
									py-3
								"
							></div>
									MTTs
								</div>
								<div
									class="
										col-lg-12 col-3
										p-2
										m-lg-3
										fs-6
										fw-bold
										subName2
									"
									id="mtts"
								>
									WIE
								</div>
								<div
									class="
										col-lg-12 col-3
										p-2
										m-lg-3
										fs-6
										fw-bold
										subName1
									"
									id="isv"
								>
									ISV
								</div>
								<div
									class="
										col-lg-12 col-3
										p-2
										m-lg-3
										fs-6
										fw-bold
										subName2
									"
									id="cs"
								>
									CS
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</section>
        <%- include('./partials/footer.ejs') %>
		<script>
			let slider = tns({
				container: '.my-slider',
				items: 1,
				slideBy: 'page',
				autoplay: true,
				mouseDrag: 'mouseDrag',
				autoplayButtonOutput: false,
				nav: false,
				controlsContainer: '#controls',
				prevButton: '.previous',
				nextButton: '.next',
				center: true,
				responsive: {
					300: {
						items: 1
					},
					570: {
						items: 2
					},
					800: {
						items: 3
					}
				}
			});

			$(window).scroll(testScroll);
			var viewed = false;

			function isScrolledIntoView(elem) {
				var docViewTop = $(window).scrollTop();
				var docViewBottom = docViewTop + $(window).height();

				var elemTop = $(elem).offset().top;
				var elemBottom = elemTop + $(elem).height();

				return elemBottom <= docViewBottom && elemTop >= docViewTop;
			}
			
			document
				.getElementById('wie')
				.classList.add('active-Sub', 'active-sub1');

			document.getElementById('wie').addEventListener('click', () => {
				changeImage(1, ['mtts', 'cs', 'isv', 'wie']);
			});
			document.getElementById('mtts').addEventListener('click', () => {
				changeImage(2, ['wie', 'cs', 'isv', 'mtts']);
			});
			document.getElementById('cs').addEventListener('click', () => {
				changeImage(3, ['wie', 'mtts', 'isv', 'cs']);
			});
			document.getElementById('isv').addEventListener('click', () => {
				changeImage(4, ['wie', 'mtts', 'cs', 'isv']);
			});
		</script>
	</body>
</html>
